{
  "openapi": "3.0.1",
  "info": {
    "title": "Zybach",
    "version": "v1"
  },
  "paths": {
    "/api/wells/pumpedVolume": {
      "get": {
        "tags": [
          "ZybachAPI"
        ],
        "summary": "Returns a time series representing pumped volume at a well or series of wells, summed daily for a given date range.\r\nEach point in the output time series represents the total pumped volume for the given day.",
        "description": "Sample requests:\r\n            \r\n    Returns data for ALL wells from 8/1/2021 to 8/31/2021\r\n    GET /api/wells/pumpedVolume?startDate=2021-08-01&endDate=2021-08-31\r\n            \r\n    Returns data for Well Registration IDs G-056157, G-097457, G-110920 from 8/1/2021 to 8/31/2021\r\n    GET /api/wells/pumpedVolume?startDate=2021-08-01&endDate=2021-08-31&wellRegistrationID=G-056157&wellRegistrationID=G-097457&wellRegistrationID=G-110920",
        "parameters": [
          {
            "name": "wellRegistrationID",
            "in": "query",
            "description": "The Well Registration ID(s) for the requested Well(s). If left blank, will bring back data for every Well that has reported data within the time range.",
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          {
            "name": "startDate",
            "in": "query",
            "description": "The start date for the report in yyyy-MM-dd format (eg. 2020-06-23)",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "endDate",
            "in": "query",
            "description": "The end date for the report in yyyy-MM-dd format (eg. 2020-06-23)",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Zybach.API.Models.ApiResult`1[Zybach.API.Models.StructuredResults]"
                }
              }
            }
          }
        }
      }
    },
    "/api/wells/{wellRegistrationID}/pumpedVolume": {
      "get": {
        "tags": [
          "ZybachAPI"
        ],
        "summary": "Returns a time series representing pumped volume at a well, summed daily for a given date range.\r\nEach point in the output time series represents the total pumped volume for the given day.",
        "description": "Sample request:\r\n\r\n    GET /api/wells/G-110920/pumpedVolume?startDate=2021-08-01&endDate=2021-08-31",
        "parameters": [
          {
            "name": "wellRegistrationID",
            "in": "path",
            "description": "The Well Registration ID for the requested Well.",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "startDate",
            "in": "query",
            "description": "The start date for the report in yyyy-MM-dd format (eg. 2020-06-23)",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "endDate",
            "in": "query",
            "description": "The end date for the report in yyyy-MM-dd format (eg. 2020-06-23)",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Zybach.API.Models.ApiResult`1[Zybach.API.Models.StructuredResults]"
                }
              }
            }
          }
        }
      }
    }
  },
