{
  "name": "zybach-web",
  "version": "0.0.0",
  "config": {
    "template_dir": "../Build/typescript-angular",
    "output_dir": "src/app/shared/generated",
    "swagger_json_path": "../Zybach.API/swagger.json"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --disable-host-check true --ssl --ssl-cert server.crt --ssl-key server.key",
    "build": "ng build",
    "build-dev": "ng build",
    "build-qa": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=qa",
    "build-ci": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=qa",
    "build-prod": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean-model": "node src/scripts/clean-model.js",
    "gen-model": "npm run clean-model %npm_package_config_output_dir% && npx @openapitools/openapi-generator-cli generate --additional-properties=modelPropertyNaming=original,fileNaming=kebab-case,useHttpClient=true,supportsES6=true,useRxJS6=true,genericModuleWithProviders=true,ngVersion=12.2.213 -i %npm_package_config_swagger_json_path% -g typescript-angular -o %npm_package_config_output_dir% -t %npm_package_config_template_dir%",
    "prestart": "@powershell if((Test-Path server.crt) -eq $false -or (Test-Path server.key) -eq $false) { npm run create-dev-cert } && npm run trust-dev-cert",
    "create-dev-cert": "openssl req -newkey rsa:2048 -x509 -nodes -keyout server.key -new -out server.crt -config ./dev-cert.cnf -sha256 -days 3650",
    "trust-dev-cert": "@powershell -Command Import-Certificate -FilePath server.crt -CertStoreLocation cert:\\LocalMachine\\Root",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/elements": "^15.2.10",
    "@angular/forms": "^15.2.10",
    "@angular/localize": "^15.2.10",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/platform-server": "^15.2.10",
    "@angular/router": "^15.2.10",
    "@microsoft/applicationinsights-web": "^2.5.10",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "@ng-select/ng-select": "^7.4.0",
    "@tinymce/tinymce-angular": "^6.0.0",
    "@turf/boolean-within": "^6.3.0",
    "@turf/helpers": "^6.3.0",
    "@types/geojson": "^7946.0.7",
    "@types/json2csv": "^5.0.1",
    "@types/leaflet": "^1.5.21",
    "@types/leaflet.icon.glyph": "^0.2.0",
    "ag-grid-angular": "^26.1.0",
    "ag-grid-community": "^26.1.0",
    "angular-mydatepicker": "^0.11.5",
    "angular-oauth2-oidc": "^12.1.0",
    "angular-oauth2-oidc-jwks": "^12.0.0",
    "bootstrap": "^4.4.1",
    "color-scheme": "^1.0.1",
    "core-js": "^3.6.5",
    "font-awesome": "^4.7.0",
    "google-palette": "^1.1.0",
    "iso8601-js-period": "^0.2.1",
    "jquery": "^3.4.1",
    "json2csv": "^5.0.6",
    "leaflet": "1.7.1",
    "leaflet-gesture-handling": "^1.2.1",
    "leaflet.fullscreen": "^2.1.0",
    "leaflet.icon.glyph": "^0.3.0",
    "leaflet.snogylop": "^0.4.0",
    "moment": "^2.29.1",
    "ngx-cookie-service": "^12.0.3",
    "ngx-mask": "^11.1.5",
    "ngx-pagination": "^5.1.1",
    "ngx-progressbar": "^6.1.1",
    "ngx-timeago": "^2.0.0",
    "ngx-toastr": "^13.2.0",
    "popper.js": "^1.16.1-lts",
    "primeng": "^12.2.2",
    "process": "^0.11.10",
    "rxjs": "^6.5.5",
    "set-value": ">=4.0.1",
    "style-loader": "^1.2.1",
