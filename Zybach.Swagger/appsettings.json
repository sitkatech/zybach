{
  "AllowedHosts": "*",
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        // "Hangfire": "Warning" // you can override SourceContext levels here
      }
    }
  }
}
